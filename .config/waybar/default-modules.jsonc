{
  "layer": "top",
  "exclusive": true,
  "passthrough": false,
  "gtk-layer-shell": true,
  "height": 20,
  "margin-top": 0,
  "margin-left": 0,
  "margin-right": 0,
  "hyprland/window": {
    "format": "{}",
    "rewrite": {
      "(.*) - YouTube Music ‚Äî Mozilla Firefox": "ü¶ä $1 üé∂",
      "(.*) ‚Äî Mozilla Firefox": "ü¶ä $1",
      "Cisco Packet Tracer - (.*)": "üíæ $1",
      "nvim (.*)": "üóíÔ∏è $1",
      "~(.*)": "üê± üè†$1",
      "(.*) - Thunar": "üìÅ $1",
      "(.*) - Visual Studio Code": "üíª $1",
      "(.*) - Discord": "üó®Ô∏è $1"
    },
    "on-click-right": "hyprctl dispatch killactive",
    "max-length": 100
  },
  "hyprland/workspaces": {
    "disable-scroll": false,
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
    "all-outputs": true,
    "on-click": "activate"
  },
  "hyprland/language": {
    "format": "ÔíÑ {}",
    "format-en": "en",
    "format-es": "es"
  },
  "tray": {
    "icon-size": 18,
    "spacing": 0
  },
  "clock": {
    "format": "{:ÔÄó %I:%M %p}",
    "tooltip-format": "<big>ÔÅ≥ {:%Y/%m/%d}</big>\n<tt><small>{calendar}</small></tt>"
  },
  "custom/notification": {
    "tooltip": false,
    "format": "{icon}",
    "format-icons": {
      "notification": "Û∞Çö<span foreground='red'><sup>ÔëÑ</sup></span>",
      "none": "Û∞Çö",
      "dnd-notification": "Û∞Çõ<span foreground='red'><sup>ÔëÑ</sup></span>",
      "dnd-none": "Û∞Çõ",
      "inhibited-notification": "Û∞Çö<span foreground='red'><sup>ÔëÑ</sup></span>",
      "inhibited-none": "Û∞Çö",
      "dnd-inhibited-notification": "Û∞Çõ<span foreground='red'><sup>ÔëÑ</sup></span>",
      "dnd-inhibited-none": "Û∞Çõ"
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t",
    "on-click-right": "swaync-client -d -sw",
    "escape": false
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "{icon} {percent}%",
    "format-icons": [
      "Û∞Éû",
      "Û∞Éü",
      "Û∞É†"
    ],
    "on-scroll-up": "brightnessctl set 1%+",
    "on-scroll-down": "brightnessctl set 1%-",
    "min-length": 6,
    "tooltip": false
  },
  "battery": {
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 20
    },
    "format": "{icon} {capacity}%",
    "format-charging": "Ôá¶ {capacity}%",
    "format-plugged": "Ôá¶ {capacity}%",
    "format-alt": "{time} {icon}",
    "format-icons": [
      "Û∞Çé",
      "Û∞Å∫",
      "Û∞Åª",
      "Û∞Åº",
      "Û∞ÅΩ",
      "Û∞Åæ",
      "Û∞Åø",
      "Û∞ÇÄ",
      "Û∞ÇÅ",
      "Û∞ÇÇ",
      "Û∞Åπ"
    ]
  },
  "pulseaudio": {
    "format": "{icon}",
    "tooltip-format": "{} | {volume}%",
    "tooltip": true,
    "format-muted": "Û∞ñÅ",
    "on-click": "pamixer -t",
    "on-scroll-up": "pamixer -i 5",
    "on-scroll-down": "pamixer -d 5",
    "scroll-step": 5,
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ]
    }
  },
  "pulseaudio#microphone": {
    "format": "{format_source}",
    "tooltip-format": "{format_source}",
    "format-source": "ÔÑ∞",
    "format-source-muted": "ÔÑ±",
    "on-click": "pamixer --default-source -t",
    "on-scroll-up": "pamixer --default-source -i 5",
    "on-scroll-down": "pamixer --default-source -d 5",
    "scroll-step": 5,
    "tooltip": false
  },
  "temperature": {
    "thermal-zone": 2,
    "format": "Ôãâ {temperatureC}¬∞C",
    "critical-threshold": 80,
    "format-critical": "Ôãá {temperatureC}¬∞C",
    "tooltip": false
  },
  "network": {
    "format-wifi": "Ôá´ {signalStrength}%",
    "format-ethernet": "Û∞àÄ {ipaddr}",
    "tooltip-format": "{ifname} via {gwaddr}",
    "format-linked": "{ifname} (No IP)",
    "format-disconnected": "‚ö† Disconnected",
    "format-alt": "{ifname}:{essid} {ipaddr}/{cidr}"
  },
  "bluetooth": {
    "format": "Ôäî on",
    "format-disabled": "Û∞Ç≤ off",
    "format-connected": "Ôäî {num_connections}",
    "tooltip-format": "{device_alias}",
    "tooltip-format-connected": "Ôäî {device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}"
  },
  "custom/playerctl": {
    "interval": 1,
    "format": "{icon} {}",
    "max-length": 40,
    "exec": "~/.local/bin/getsong -json",
    "on-click": "playerctl play-pause",
    "on-scroll-up": "playerctl next",
    "on-scroll-down": "playerctl previous",
    "smooth-scrolling-threshold": 10,
    "return-type": "json",
    "format-icons": {
      "Playing": "Û∞éà",
      "Paused": "Û∞è§"
    }
  },
  "pulseaudio/slider": {
    "min": 0,
    "max": 100,
    "orientation": "horizontal"
  },
  "backlight/slider": {
    "min": 0,
    "max": 100,
    "orientation": "horizontal",
    "device": "intel_backlight"
  },
  "group/light": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "transition-left-to-right": false
    },
    "modules": [
      "backlight",
      "backlight/slider"
    ]
  },
  //FOR POWER GROUP
  "custom/arch": {
    "format": "Û∞£á",
    "tooltip": false,
    "on-click": "wofi"
  },
  "custom/quit": {
    "format": "Û∞çÉ",
    "tooltip": false,
    "on-click": "if [ $(notify-send -e -w -u critical -A 'Logout' -A 'Cancel' -i $(busctl get-property org.freedesktop.Accounts /org/freedesktop/Accounts/User$UID org.freedesktop.Accounts.User IconFile | grep -o '\".*\"' | tr -d '\"') 'Bye bye?' 'Are you sure you want to logout?') -eq 0 ]; then killall -9 Hyprland; fi"
  },
  "custom/lock": {
    "format": "Û∞åæ",
    "tooltip": false,
    "on-click": "swaylock"
  },
  "custom/reboot": {
    "format": "Û∞úâ",
    "tooltip": false,
    "on-click": "if [ $(notify-send -e -w -u critical -A 'Reboot' -A 'Cancel' -i $(busctl get-property org.freedesktop.Accounts /org/freedesktop/Accounts/User$UID org.freedesktop.Accounts.User IconFile | grep -o '\".*\"' | tr -d '\"') 'Bye bye?' 'Are you sure you want to reboot the PC?') -eq 0 ]; then systemctl reboot; fi",
    "on-click-right": "systemctl reboot"
  },
  "custom/shutdown": {
    "format": "Û∞ê•",
    "tooltip": false,
    "on-click": "if [ $(notify-send -e -w -u critical -A 'Power off' -A 'Cancel' -i $(busctl get-property org.freedesktop.Accounts /org/freedesktop/Accounts/User$UID org.freedesktop.Accounts.User IconFile | grep -o '\".*\"' | tr -d '\"') 'Bye bye?' 'Are you sure you want to power off the PC?') -eq 0 ]; then systemctl poweroff; fi",
    "on-click-right": "systemctl poweroff"
  },
  "custom/sleep": {
    "format": "Û∞í≤",
    "tooltip": false,
    "on-click": "systemctl suspend"
  },
  //GROUP CONFIGURATIONS
  "group/audioSpeaker": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "transition-left-to-right": false
    },
    "modules": [
      "pulseaudio",
      "pulseaudio/slider"
    ]
  },
  "group/audio": {
    "orientation": "inherit",
    "modules": [
      "group/audioSpeaker",
      "pulseaudio#microphone"
    ]
  },
  "group/power": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "transition-left-to-right": false
    },
    "modules": [
      "custom/arch",
      "custom/quit",
      "custom/lock",
      "custom/sleep",
      "custom/reboot",
      "custom/shutdown"
    ]
  }
}
